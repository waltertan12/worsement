{
   "name": "portfolio-manager",
   "version": "1.0.0",
   "main": "index.js",
   "license": "MIT",
   "devDependencies": {
      "@babel/core": "^7.10.1",
      "@babel/preset-env": "^7.10.1",
      "@babel/preset-typescript": "^7.10.1",
      "@types/compression": "^1.7.0",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.6",
      "@types/express-session": "^1.17.0",
      "@types/helmet": "^0.0.47",
      "@types/jest": "^25.2.3",
      "@types/morgan": "^1.9.0",
      "@types/node": "^8.0.29",
      "@types/passport": "^1.0.3",
      "@types/passport-oauth2": "^1.4.9",
      "@types/ramda": "^0.27.34",
      "@typescript-eslint/eslint-plugin": "^3.0.0",
      "@typescript-eslint/parser": "^3.0.0",
      "babel-jest": "^26.0.1",
      "concurrently": "^5.2.0",
      "eslint": "^7.1.0",
      "eslint-config-prettier": "^6.11.0",
      "eslint-plugin-prettier": "^3.1.3",
      "husky": "^4.3.7",
      "jest": "^26.0.1",
      "lint-staged": "^10.5.3",
      "nodemon": "^2.0.7",
      "prettier": "^2.0.5",
      "ts-jest": "^26.5.5",
      "ts-node": "3.3.0",
      "typescript": "^4.2.4"
   },
   "dependencies": {
      "@alpacahq/alpaca-trade-api": "^1.3.0",
      "@types/ws": "^7.2.4",
      "axios": "^0.21.1",
      "compression": "^1.7.4",
      "cors": "^2.8.5",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-prom-bundle": "^6.0.0",
      "express-session": "^1.17.1",
      "helmet": "^3.22.0",
      "morgan": "^1.10.0",
      "mysql": "^2.14.1",
      "passport": "^0.4.1",
      "passport-oauth2": "^1.5.0",
      "pg": "^8.6.0",
      "prom-client": "^12.0.0",
      "ramda": "^0.27.1",
      "reflect-metadata": "^0.1.10",
      "typeorm": "0.2.32",
      "upgrade": "^1.1.0",
      "ws": "^7.3.0"
   },
   "scripts": {
      "build": "tsc",
      "migration:generate": "TS_NODE=1 ts-node ./node_modules/typeorm/cli.js migration:generate",
      "migration:revert": "TS_NODE=1 ts-node ./node_modules/typeorm/cli.js migration:revert",
      "migration:run": "TS_NODE=1 ts-node ./node_modules/typeorm/cli.js migration:run",
      "format": "prettier --write src/",
      "lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
      "start": "cd dist && node dist/index.js",
      "start:ts": "ts-node src/index.ts",
      "test": "jest",
      "test:watch": "jest --watch",
      "watch": "concurrently \"tsc --watch\" \"nodemon dist/index.js\""
   },
   "husky": {
      "hooks": {
         "pre-commit": "npm run format && npm run lint"
      },
      "lint-staged": {
         "*.{ts,tsx,json}": [
            "npm run lint",
            "npm run format"
         ]
      }
   }
}
